{
    "name": "@zenstackhq/tanstack-query",
    "displayName": "ZenStack plugin for generating tanstack-query hooks",
    "version": "2.10.1",
    "description": "ZenStack plugin for generating tanstack-query hooks",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "typings": "dist/index.d.ts",
    "files": ["dist"],
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.mts",
                "default": "./dist/index.mjs"
            },
            "require": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            }
        },
        "./runtime": {
            "import": {
                "types": "./dist/runtime/index.d.mts",
                "default": "./dist/runtime/index.mjs"
            },
            "require": {
                "types": "./dist/runtime/index.d.ts",
                "default": "./dist/runtime/index.js"
            }
        },
        "./runtime/react": {
            "import": {
                "types": "./dist/runtime/react.d.mts",
                "default": "./dist/runtime/react.mjs"
            },
            "require": {
                "types": "./dist/runtime/react.d.ts",
                "default": "./dist/runtime/react.js"
            }
        },
        "./runtime/vue": {
            "import": {
                "types": "./dist/runtime/vue.d.mts",
                "default": "./dist/runtime/vue.mjs"
            },
            "require": {
                "types": "./dist/runtime/vue.d.ts",
                "default": "./dist/runtime/vue.js"
            }
        },
        "./runtime/svelte": {
            "import": {
                "types": "./dist/runtime/svelte.d.mts",
                "default": "./dist/runtime/svelte.mjs"
            },
            "require": {
                "types": "./dist/runtime/svelte.d.ts",
                "default": "./dist/runtime/svelte.js"
            }
        },
        "./runtime-v5": {
            "import": {
                "types": "./dist/runtime-v5/index.d.mts",
                "default": "./dist/runtime-v5/index.mjs"
            },
            "require": {
                "types": "./dist/runtime-v5/index.d.ts",
                "default": "./dist/runtime-v5/index.js"
            }
        },
        "./runtime-v5/react": {
            "import": {
                "types": "./dist/runtime-v5/react.d.mts",
                "default": "./dist/runtime-v5/react.mjs"
            },
            "require": {
                "types": "./dist/runtime-v5/react.d.ts",
                "default": "./dist/runtime-v5/react.js"
            }
        },
        "./runtime-v5/vue": {
            "import": {
                "types": "./dist/runtime-v5/vue.d.mts",
                "default": "./dist/runtime-v5/vue.mjs"
            },
            "require": {
                "types": "./dist/runtime-v5/vue.d.ts",
                "default": "./dist/runtime-v5/vue.js"
            }
        },
        "./runtime-v5/svelte": {
            "import": {
                "types": "./dist/runtime-v5/svelte.d.mts",
                "default": "./dist/runtime-v5/svelte.mjs"
            },
            "require": {
                "types": "./dist/runtime-v5/svelte.d.ts",
                "default": "./dist/runtime-v5/svelte.js"
            }
        },
        "./package.json": "./package.json"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/zenstackhq/zenstack"
    },
    "scripts": {
        "clean": "rimraf dist",
        "build": "pnpm lint --max-warnings=0 && tsup-node && node scripts/postbuild && pnpm pack --pack-destination ../../../.build",
        "watch": "tsup-node --watch",
        "lint": "eslint src --ext ts",
        "test": "NODE_OPTIONS=\"$NODE_OPTIONS --experimental-vm-modules\" jest --testPathPattern=\"test\\.ts$\"; jest --testPathPattern=\"test\\.tsx$\"",
        "prepublishOnly": "pnpm build"
    },
    "keywords": [],
    "author": "ZenStack Team",
    "license": "MIT",
    "dependencies": {
        "@zenstackhq/runtime": "workspace:*",
        "@zenstackhq/sdk": "workspace:*",
        "change-case": "^4.1.2",
        "cross-fetch": "^4.0.0",
        "lower-case-first": "^2.0.2",
        "semver": "^7.5.2",
        "ts-morph": "^16.0.0",
        "ts-pattern": "^4.3.0",
        "upper-case-first": "^2.0.2"
    },
    "devDependencies": {
        "@tanstack/react-query": "^4.29.7",
        "@tanstack/react-query-v5": "npm:@tanstack/react-query@5.56.x",
        "@tanstack/svelte-query": "^4.29.7",
        "@tanstack/svelte-query-v5": "npm:@tanstack/svelte-query@^5.0.0",
        "@tanstack/vue-query": "^4.37.0",
        "@testing-library/react": "14.0.0",
        "@types/react": "18.2.0",
        "@types/semver": "^7.3.13",
        "@types/tmp": "^0.2.3",
        "@zenstackhq/testtools": "workspace:*",
        "jest-environment-jsdom": "^29.7.0",
        "nock": "^13.3.6",
        "react": "18.2.0",
        "react-test-renderer": "^18.2.0",
        "replace-in-file": "^7.0.1",
        "svelte": "^4.2.1",
        "swr": "^2.0.3",
        "tmp": "^0.2.3",
        "vue": "^3.3.4"
    }
}
