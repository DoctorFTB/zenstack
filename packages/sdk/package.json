{
    "name": "@zenstackhq/sdk",
    "version": "2.10.1",
    "description": "ZenStack plugin development SDK",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "typings": "dist/index.d.ts",
    "scripts": {
        "lint": "eslint src --ext ts",
        "build": "pnpm lint --max-warnings=0 && tsup-node && pnpm pack --pack-destination ../../.build",
        "watch": "tsup-node --watch",
        "prepublishOnly": "pnpm build"
    },
    "files": ["dist"],
    "keywords": [],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "@prisma/generator-helper": "6.0.x",
        "@prisma/internals": "6.0.x",
        "@zenstackhq/language": "workspace:*",
        "@zenstackhq/runtime": "workspace:*",
        "langium": "1.3.1",
        "lower-case-first": "^2.0.2",
        "semver": "^7.5.2",
        "ts-morph": "^16.0.0",
        "ts-pattern": "^4.3.0",
        "upper-case-first": "^2.0.2"
    },
    "devDependencies": {
        "@types/semver": "^7.3.13"
    },
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.mts",
                "default": "./dist/index.mjs"
            },
            "require": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            }
        },
        "./ast": {
            "import": {
                "types": "./dist/ast.d.mts",
                "default": "./dist/ast.mjs"
            },
            "require": {
                "types": "./dist/ast.d.ts",
                "default": "./dist/ast.js"
            }
        },
        "./prisma": {
            "import": {
                "types": "./dist/prisma.d.mts",
                "default": "./dist/prisma.mjs"
            },
            "require": {
                "types": "./dist/prisma.d.ts",
                "default": "./dist/prisma.js"
            }
        },
        "./dmmf-helpers": {
            "import": {
                "types": "./dist/dmmf-helpers/index.d.mts",
                "default": "./dist/dmmf-helpers/index.mjs"
            },
            "require": {
                "types": "./dist/dmmf-helpers/index.d.ts",
                "default": "./dist/dmmf-helpers/index.js"
            }
        },
        "./package.json": "./package.json"
    }
}
